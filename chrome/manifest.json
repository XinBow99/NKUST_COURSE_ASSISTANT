{
  "name": "NKUST HELPER",
  "description": "NKUST HELPER!",
  "version": "2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mobile.nkust.edu.tw/*"],
      "css": ["bootstrap.min.css"],
      "js": ["bootstrap.bundle.min.js", "jquery.js", "content.js","swal.js"]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>",
    "cookies",
    "contextMenus",
    "storage",
    "webNavigation"
  ]
}
