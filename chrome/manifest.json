{
  "name": "高科小助手",
  "description": "幫助你快速取得所有課程，並做出分類",
  "version": "15.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mobile.nkust.edu.tw/*"],
      "css": ["bootstrap.min.css"],
      "js": ["bootstrap.bundle.min.js", "jquery.js", "content.js","swal.js"]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>",
    "cookies"
  ]
}
